<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <!-- Using Xml File -->
  <!-- to specify dispatcher servlet class -->
  <servlet>
  	<servlet-name>FirstDispatcher</servlet-name>
  	<!-- To Provide FQNC of Dispatcher Servlet Class -->  
  	<servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
  </servlet>
  <!-- Mapping - Which Request should get Handle by Dispatcher Servlet --> 
  <servlet-mapping>
  	<servlet-name>FirstDispatcher</servlet-name>
  	<url-pattern>/</url-pattern>  <!-- Handle all the type of Request --> 
  	 <!-- <url-pattern>/demo</url-pattern> Handle Request which Contains demo  -->
  </servlet-mapping> 
  <!-- <servlet-mapping>
  	<servlet-name>FirstDispatcher</servlet-name>
  	<url-pattern>*.css</url-pattern>
  </servlet-mapping> -->
  <!-- <servlet-mapping>
  	<servlet-name>FirstDispatcher</servlet-name>
  	<url-pattern>*.html</url-pattern>
  </servlet-mapping> -->
</web-app>
